# Apibara Indexer Configuration

# Stream URLs
MAINNET_STREAM_URL=https://mainnet.starknet.a5a.ch
SEPOLIA_STREAM_URL=https://sepolia.starknet.a5a.ch

# Redis persistence
PERSIST_TO_REDIS=redis://localhost:6379/0

# Tokenkit Indexer Configuration
TOKENKIT_MAINNET_INDEXER_ID=tokenkit-mainnet
TOKENKIT_MAINNET_STARTING_BLOCK=0
TOKENKIT_MAINNET_WEBSOCKET_ENDPOINT=ws://localhost:8000/ws/tokenkit/
TOKENKIT_MAINNET_CONTRACT_ADDRESS=0x123...

TOKENKIT_SEPOLIA_INDEXER_ID=tokenkit-sepolia
TOKENKIT_SEPOLIA_STARTING_BLOCK=0
TOKENKIT_SEPOLIA_WEBSOCKET_ENDPOINT=ws://localhost:8000/ws/tokenkit-sepolia/
TOKENKIT_SEPOLIA_CONTRACT_ADDRESS=0x123...

# Transfers Indexer Configuration
TRANSFERS_MAINNET_INDEXER_ID=transfers-mainnet
TRANSFERS_MAINNET_STARTING_BLOCK=0
TRANSFERS_MAINNET_WEBSOCKET_ENDPOINT=ws://localhost:8000/ws/transfers/

TRANSFERS_SEPOLIA_INDEXER_ID=transfers-sepolia
TRANSFERS_SEPOLIA_STARTING_BLOCK=0
TRANSFERS_SEPOLIA_WEBSOCKET_ENDPOINT=ws://localhost:8000/ws/transfers-sepolia/

# WebSocket Configuration
# Delay in milliseconds before sending data to the WebSocket
# This helps control the rate at which data is sent to the backend
WEBSOCKET_DELAY_MS=0
