{
  "name": "apibara-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "apibara prepare",
    "transfers:mainnet": "apibara dev --preset mainnet-transfers --indexers transfers",
    "transfers:sepolia": "apibara dev --preset sepolia-transfers --indexers transfers",
    "tokenkit:mainnet": "apibara dev --preset mainnet-tokenkit --indexers tokenkit",
    "tokenkit:sepolia": "apibara dev --preset sepolia-tokenkit --indexers tokenkit",
    "start:transfers:mainnet": "apibara start --preset mainnet-transfers --indexer transfers",
    "start:transfers:sepolia": "apibara start --preset sepolia-transfers --indexer transfers",
    "start:tokenkit:mainnet": "apibara start --preset mainnet-tokenkit --indexer tokenkit",
    "start:tokenkit:sepolia": "apibara start --preset sepolia-tokenkit --indexer tokenkit",
    "start": "apibara start",
    "build": "apibara build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apibara/indexer": "next",
    "@apibara/protocol": "next",
    "@apibara/starknet": "next",
    "@types/ws": "^8.18.1",
    "apibara": "next",
    "ioredis": "^5.6.1",
    "kafkajs": "^2.2.4",
    "starknet": "^6.24.1",
    "superjson": "^2.2.2",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.1",
    "dotenv-cli": "^8.0.0",
    "typescript": "^5.8.3"
  }
}
