# Kafka configuration for indexers
KAFKA_BROKERS=localhost:9093

# Tenant schemas - these are fixed values that match the Django tenant schemas
KAFKA_MAINNET_TENANT_SCHEMA=mainnet
KAFKA_SEPOLIA_TENANT_SCHEMA=sepolia

# Kafka topics for transfers indexer
KAFKA_TOPIC_MAINNET_TRANSFERS=tokenkit-transfers-mainnet
KAFKA_TOPIC_SEPOLIA_TRANSFERS=tokenkit-transfers-sepolia
KAFKA_CLIENT_ID_TRANSFERS=tokenkit-transfers-indexer

# Kafka topics for tokenkit contract indexer
KAFKA_TOPIC_MAINNET_TOKENKIT=tokenkit-contract-mainnet
KAFKA_TOPIC_SEPOLIA_TOKENKIT=tokenkit-contract-sepolia
KAFKA_CLIENT_ID_TOKENKIT=tokenkit-tokenkit-indexer

# Add these to your existing .env file
